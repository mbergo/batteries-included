{
  "files.insertFinalNewline": true,
  "files.exclude": {
    "**/.git": true,
    "**/.sl": true,
    "**/.jj": true,
    "**/.DS_Store": true,
    "**/.direnv": true,
    "**/.nix-hex": true,
    "**/.nix-cargo": true,
    "**/.nix-go": true,
    "**/.nix-mix": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/deps": true
  },
  "files.watcherExclude": {
    "**/.git/**": true,
    "**/.sl/**": true,
    "**/.jj/**": true,
    "**/.direnv/**": true,
    "**/.nix-*/**": true,
    "**/node_modules/**": true,
    "**/_build/**": true,
    "**/deps/**": true
  },
  "elixirLS.projectDir": "platform_umbrella",
  "elixirLS.fetchDeps": true,
  "elixirLS.mixEnv": "dev",
  "elixirLS.mixTarget": "",
  "elixirLS.dialyzerEnabled": false
}
